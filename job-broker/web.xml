<?xml version="1.0"?>
<web-app>
<display-name>job-broker</display-name>
<description>Coder Contest - Job Broker</description>

<listener>
 <listener-class>dragonfin.contest.broker.ContextListener</listener-class>
</listener>

<servlet>
 <servlet-name>TestFeed</servlet-name>
 <servlet-class>dragonfin.contest.broker.TestFeedServlet</servlet-class>
</servlet>
<servlet-mapping>
 <servlet-name>TestFeed</servlet-name>
 <url-pattern>/feed</url-pattern>
</servlet-mapping>

<servlet>
 <servlet-name>Notify</servlet-name>
 <servlet-class>dragonfin.contest.broker.NotifyServlet</servlet-class>
</servlet>
<servlet-mapping>
 <servlet-name>Notify</servlet-name>
 <url-pattern>/notify</url-pattern>
</servlet-mapping>

</web-app>
